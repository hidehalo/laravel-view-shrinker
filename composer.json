{
    "name": "hidehalo/laravel-view-shrinker",
    "description": "Laravel framework view shrinker",
    "type": "library",
    "require": {
        "laravel/framework": "^6|^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "authors": [
        {
            "name": "hidehalo",
            "email": "tianchen_cc@yeah.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hidehalo\\View\\Compilers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hidehalo\\View\\Compilers\\Tests\\": "tests"
        }
    },
    "archive": {
        "exclude": [
            "tests/",
            ".gitignore",
            "*.md",
            "*.yml",
            "*.xml"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hidehalo\\View\\Compilers\\ShrinkCompilerServiceProvider"
            ]
        }
    },
    "license": "MIT"
}
